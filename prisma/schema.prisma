
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator json {
  provider = "prisma-json-types-generator"
}



model User {
  id             Int         @unique @default(autoincrement())
  email          String      @id @unique
  password       String
  socialName     String      @unique
}

